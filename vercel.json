{
  "functions": {
    "api/appointment-confirmation/index.js": {
      "memory": 256,
      "maxDuration": 10
    },
    "api/business-notification/index.js": {
      "memory": 256,
      "maxDuration": 10
    },
    "api/edge-config/store/index.js": {
      "memory": 256,
      "maxDuration": 10
    },
    "api/edge-config/verify/[code]/index.js": {
      "memory": 256,
      "maxDuration": 10
    },
    "api/edge-config/confirm/[code]/index.js": {
      "memory": 256,
      "maxDuration": 10
    },
    "api/edge-config/availability/index.js": {
      "memory": 256,
      "maxDuration": 10
    }
  },
  "regions": ["iad1"],
  "headers": [
    {
      "source": "/api/(appointment-confirmation|business-notification|edge-config)/**",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "https://juanzhingre.com,https://www.juanzhingre.com,http://localhost:2509" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,POST,OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type,Authorization" }
      ]
    }
  ]
}